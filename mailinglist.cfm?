	<!--[if IE 6]>
	<script type="text/javascript" src="/common/js/jquery/plugins/pngFix/jquery.pngFix.js"></script> 
	<style type="text/css">
		.navMenuHorizontal li { margin: 0 5px; } // add in the spacing that seems to disappear
	</style>
	<script type="text/javascript">
		jQuery(document).ready(function($){
			$('.subtitlewrap').css('position', 'relative'); // make sutitles position relative (so the next step works)
			// replace the background images on the nav with actual images - otherwise 
			// if we do a fix on both the BG & the image then it breaks the links
			$('.navMenu li.top, .subtitlewrap').each(function() {
				var bgIMG = $(this).css('background-image');
				if(bgIMG != null && bgIMG.indexOf(".png")!=-1){
					var iebg = bgIMG.split('url("')[1].split('")')[0];
					$t = $(this);
					$t.css('background-image', 'none');
					$t.prepend( '<div style="overflow: hidden; display: block; position: absolute; top: 0; left: 0; height: ' + $t.height() + 'px; width: 100%; text-align: left;"><img src="' + iebg + '" /></div>' );
				}
			});
			// fix the pngs
			$('.navMenu, .subtitlewrap').pngFix({doBg:false});
		});
	</script>
	<![endif]-->

	
		<script type="text/javascript" src="/common/js/zoogle/framed_musicplayer.js?2"></script>
		<script language="JavaScript" type="text/JavaScript">
			
				
				if (isMusicPlayerOpen()) closeFramedMusicPlayer();
			
		</script>
	
			<script type="text/javascript" src="/common/js/jquery/plugins/jquery.prompting_input.js"></script>
			<script type="text/javascript" src="/common/js/jquery/plugins/jquery-validate/lib/jquery.form.js"></script>
			<script type="text/javascript" src="/common/js/jquery/plugins/bgiframe/jquery.bgiframe.js?100"></script>
			<script type="text/javascript" src="/common/js/zoogle/features/mailinglist/signup.js?100"></script>
			
			<link href="/common/usersite/css/features/mailinglist.css?1" rel="stylesheet" type="text/css" />
		
		<script type="text/javascript" src="/common/js/jquery/plugins/bgiframe/jquery.bgiframe.js"></script>
		<script type="text/javascript" src="/common/js/zoogle/jQuery.zoogleNav.js?1"></script>
		
		<script type="text/javascript">
			jQuery(document).ready(function($) {
				$('.navMenu').zoogleNav({imageSuffix: 'png'});

				// replace all the flash elements with ones with wmode params so that the
				// menu doesn't overlay on other Windows browsers (as IE uses bgiframe)
				// could extract this to be re-usable
				if( !$.browser.msie && window.navigator.userAgent.match( /Windows/ ) ) {
					$('object').each(function() {
						$this = $(this);
						if( ! $('param[name=wmode]', $this ).length == 1 ) {
							$targ = $this.clone();
							$targ.append('<param name="wmode" value="opaque" />');
							$('embed', $targ).attr('wmode', 'opaque');
							$this.replaceWith($targ);
						}
					} );

					$('embed').each(function() {
						$this = $(this);
						if( $this.attr('wmode') == null ) {
							$this.replaceWith($this.clone().attr('wmode', 'opaque'));
						}
					});
				}
			});
		</script>
	

