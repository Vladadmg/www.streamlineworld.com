	<input type="image" src="/common/shoppingcart/button-buynow.gif" border="0" align="absmiddle" name="submit" alt="Checkout" onClick="if (!updateCart(cartForm)) { alert('Use the ADD TO CART buttons above to select the tracks you wish to purchase.'); return false;} else return true;">
	</form>

	<script type="text/javascript">
		var so_blankimage = "/common/shoppingcart/blank.gif";
		var so_offimage = "/common/shoppingcart/button-addtocart.gif";
		var so_onimage = "/common/shoppingcart/button-incart.gif";

		var item_info = {
			
			'239556': {item_number: '239556', item_name: 'All I Need', amount: '0.99'}, 
			'239557': {item_number: '239557', item_name: 'Higher', amount: '0.99'}, 
			'239558': {item_number: '239558', item_name: 'Flicker', amount: '0.99'}, 
			'239559': {item_number: '239559', item_name: 'The Meaning of Life', amount: '0.99'}, 
			'239560': {item_number: '239560', item_name: 'Solace', amount: '0.99'}, 
			'239561': {item_number: '239561', item_name: 'The Untitled', amount: '0.99'}, 
			'239562': {item_number: '239562', item_name: 'The Only Thing That Matters', amount: '0.99'} 
		};
		var num_items = 7;

		function updateCart(cartForm) {
			// Clear out what's there
			for (var i = 1; i <= num_items; i++) {
				cartForm['item_number_' + i].value = '';
				cartForm['item_name_' + i].value = '';
				cartForm['amount_' + i].value = '';
			}
			// Enter in the values for only the elements that are selected.
			var last_item = 0;
			for (var item in item_info) {
				if (cartForm['addtocart_' + item].checked) {
					last_item = last_item + 1;
					cartForm['item_number_' + last_item].value = item_info[item]['item_number'];
					cartForm['item_name_' + last_item].value = item_info[item]['item_name'];
					cartForm['amount_' + last_item].value = item_info[item]['amount'];
				}
			}
			return (last_item > 0);
		}

	</script>

	<script type="text/javascript" src="/common/shoppingcart/custom_checkbox.js"></script>

	<script type="text/javascript">
		// Use the Prototype load event instead of what was in custom_checkbox.js so it does not conflict with other components.
		Event.observe(window, 'load', so_init);
	</script>




